// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model BpiSubject {
  id String @id @default(uuid())
  name String
  lastName String  
  description String  
  type Int
  publicKey String 
}