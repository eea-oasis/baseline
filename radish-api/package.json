{
  "name": "radish34-api",
  "version": "1.0.0",
  "description": "Organization API for Radish34 project",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "nodemon ./build/index.js",
    "build": "babel ./src -d ./build --copy-files",
    "watch": "babel ./src -d ./build --copy-files --watch",
    "test": "jest --watchAll --verbose=true --silent",
    "test:ci": "jest --verbose=true --silent --ci --coverage",
    "fix": "npm run format && npm run lint -- --fix",
    "format": "prettier --write \"**/*.{json,css,scss,md}\"",
    "lint": "eslint .",
    "lint:md": "markdownlint -i node_modules -i build .",
    "validate": "npm run lint && npm run lint:md"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.9.3",
    "express": "^4.17.1",
    "mongodb": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "eslint": "^5.3.0",
    "eslint-config-codfish": "^2.5.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.0",
    "markdownlint-cli": "^0.15.0",
    "faker": "git+https://git@github.com/Marak/faker.js.git#d3ce6f1",
    "nodemon": "^1.18.9",
    "prettier": "1.16.1",
    "solhint": "2.0.0-alpha.3",
    "supertest": "^3.4.2"
  }
}
